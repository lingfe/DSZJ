/* pages/myDrippingWater/dripInvitation/dripInvitation.wxss */

@import '/pages/myDrippingWater/myLaunch/myLaunch.wxss';

page{
  background-color: #fff;
}

/* 滴水请柬 start */
.dripQifu_page_model_content_text{
  font-size: 26rpx;
  width:94%;
  margin:auto;
}


/* 滴水请柬 end */


/* 线上礼单 start */
.page_model{
  width: 100%;
  margin: auto;
  border-bottom: 1rpx solid #bbb;
}
.page_model_text{
  width: 94%;
  margin: auto;
  height: 80rpx;
  text-align: center;
}
.page_model_text_text{
  font-size: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 8%;
  float: left;
}
.page_model_text_text:first-of-type{
  text-align: left;
  margin-left: 0%;
}
.page_model_invitationText{
  width: 94%;
  margin: auto;
  font-size: 30rpx;
}
.page_model_remark{
  width: 94%;
  margin: auto;
  font-size: 30rpx;
}
.page_text{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  color: #bbb;
  font-size: 28rpx;
  text-align: center;
}
.text_bottom{
  padding-bottom: 120rpx;
}
.page_xinxia_buttons{
  width: 100%;
  position: fixed;
  bottom: 0rpx;
  height: 120rpx;
  border-top: 1rpx solid #bbb;
  background-color:#fff;
}
.page_xinxia_buttons_btn{
  width:40%;
  height:80rpx;
  line-height:80rpx;
  margin:auto;
  background-color:#0099ff;
  border-radius:50rpx;
  text-align:center;
  font-size:32rpx;
  color:#fff;
  margin-top:20rpx;
}
.page_xinxia_buttons_btn:active{
  opacity: 0.5;
}
/* 线上礼单 end */

/*礼金提现 start */
.page_lijingtix_title{
  width: 100%;
  height: 120rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #bbb;
  border-top: 1rpx solid #bbb;
  margin-top: 10rpx;
}
.page_lijingtix_bg{
  width: 100%;
  height: 20rpx;
  background-color: #efeff4;
}
.page_lijingtix_title_zhoshu{
  width: 30%;
  height: 120rpx;
  font-size: 28rpx;
  float: left;
  text-align: center;
}
.page_lijingtix_title_zhoLj{
  width: 69%;
  border-left: 1rpx solid #bbb;
  height: 120rpx;
  font-size: 28rpx;
  float: left;
  text-align: center;
}
.text_zho{
  color: #ffb514;
  font-size: 40rpx;
}

.page_lijingtix_model{
  width: 100%;
  border-top: 1rpx solid #bbb;
  border-bottom: 1rpx solid #bbb;
}
.page_lijingtix_model_title{
  width: 94%;
  margin: auto;
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.page_lijingtix_model_content{
  width: 100%;
  border-top: 1rpx solid #bbb;
}
.page_lijingtix_model_content_text{
  width: 100%;
  font-size: 28rpx;
  height: 60rpx;
}
.page_lijingtix_model_content_text_title{
  width: 30%;
  font-size: 28rpx;
  height: 60rpx;
  line-height:60rpx;
  float: left;
  text-align: right;
}
.page_lijingtix_model_content_text_text{
  width: 70%;
  font-size: 28rpx;
  height: 60rpx;
  line-height:60rpx;
  float: left;
  text-align: left;
}

.page_lijingtix_model_content_input{
  width: 100%;
  font-size: 28rpx;
  height: 60rpx;
}
.page_lijingtix_model_content_input_title{
  width: 30%;
  font-size: 28rpx;
  height: 60rpx;
  line-height:60rpx;
  float: left;
  text-align: right;
}
.page_lijingtix_model_content_input_input{
  width: 66%;
  font-size: 28rpx;
  height: 60rpx;
  line-height:60rpx;
  float: left;
  text-align: left;
}
.page_lijingtix_model_content_input_input input{
  border-bottom: 1rpx solid #bbb;
}
.page_lijingtix_text{
  width: 94%;
  margin: auto;
  font-size: 30rpx;
}
.page_lijingtix_btn{
  width: 100%;
  height: 80rpx;
  border-top: 1rpx solid #bbb;
  margin-top: 20rpx;
}
.page_lijingtix_btn_btn{
   position: relative;
  display: table-cell;
  width: 50%;
  text-align: center;
  font-size: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  float: left;
  color: #0099ff;
}
.page_lijingtix_btn_btn:first-of-type::after {
    content: "";
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    width: 1px;
    background: #bbb;
}