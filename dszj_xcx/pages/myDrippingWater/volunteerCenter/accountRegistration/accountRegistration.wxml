<!--pages/myDrippingWater/volunteerCenter/accountRegistration/accountRegistration.wxml-->
<view class='page'>
  <view class='page_imageBg'></view>
  <form bindsubmit="qf_formSubmit">

            <!-- 手机号码  start-->
            <view class='page_Fundraising_title'>
              <view class='page_Fundraising_title_text'>
                <view class='page_Fundraising_title_text_title'>手机号码</view>
              </view>
              <view class='page_Fundraising_title_input input2'>
                <input type='text' name="mobile" value='{{qf_form.mobile}}' placeholder='请输入手机号码'></input>
              </view>
            </view>
            <!-- 手机号码   end-->

            
            <!-- 确认手机号码  start-->
            <view class='page_Fundraising_title'>
              <view class='page_Fundraising_title_text'>
                <view class='page_Fundraising_title_text_title'>确认手机号码</view>
              </view>
              <view class='page_Fundraising_title_input input2'>
                <input type='text' name="mobile" value='{{qf_form.mobile}}' placeholder='请确认手机号码'></input>
              </view>
            </view>
            <!-- 确认手机号码  end-->

            <view class='page_text red'>密码默认为6个"0"</view>
            <view class='page_text'>注:帐号添加成功后会自动切换成您添加的帐号并跳转到求助发起页面,案例将自动统计到志愿者帐户下。</view>
            <!-- 注册 start -->
            <view class="weui-btn-area">
              <button class="weui-btn" formType="submit" type="primary">注册</button>
            </view>
            <!-- 注册  end -->
  </form>
</view>
