/* pages/myDrippingWater/myLaunch/myLaunch.wxss */

@import '/pages/dripLove/dripLove.wxss';
@import '/pages/index/largeIllnessRescue/largeIllnessRescue.wxss';


.page_model_title {
  padding-top: 10rpx;
}
.page_model_content_right {
  height: auto;
}
.page_model_content_right_text {
  height: 50rpx;
  line-height: 50rpx;
}
.page_model_content_left {
  height: 210rpx;
}
.page_model_content_left image {
  width:30%;
  margin:auto;
  height:200rpx;
  margin-left:2.4%;
  float: left;
}
.page_model_content_right_text_text{
  word-break:keep-all;/* 不换行 */
  white-space:nowrap;/* 强制在同一行内显示所有文本，直到文本结束或者遭遇 br 对象。不换行 */
  overflow:hidden;/* 内容超出宽度时隐藏超出部分的内容 */
}

.page_model_content_caozhou{
  width: 100%;
  height: 100rpx;
}
.page_model_content_caozhou_image{
  width: 33%;
  height: 100rpx;
  float: left;
  text-align: center;
}
.image_width{
  width: 24%;
}

.page_model_content_caozhou_image image{
  width: 40%;
  height: 100rpx;
}
.image_image{
  width: 64%;
  height: 100rpx;
}


.page_model_content_btn{
    color: #fff;
    border: 1px solid #dd524d;
    background-color: #dd524d;
    font-size: 32rpx;
    display: block;
    height: 80rpx;
    line-height:80rpx;
    text-align:center;
    width:90%;
    margin:auto;
}

.dripQifu_page_model {
  margin-bottom:20rpx;
  padding-bottom:20rpx;
  margin-top:20rpx;

}
.dripQifu_page_model_content{
  height: auto;
}
.dripQifu_page_model_content_right {
  height: auto;
}
.dripQifu_page_model_content_right_text_text{
  word-break:keep-all;/* 不换行 */
  white-space:nowrap;/* 强制在同一行内显示所有文本，直到文本结束或者遭遇 br 对象。不换行 */
  overflow:hidden;/* 内容超出宽度时隐藏超出部分的内容 */
}